{
  "name": "alexmasterov/genkgo-mail-bundle",
  "description": "Bundle to integrate Genkgo Mail with Symfony projects",
  "type": "symfony-bundle",
  "license": "MIT",
  "homepage": "https://github.com/AlexMasterov/genkgo-mail-bundle",
  "authors": [{
    "name": "Alex Masterov",
    "email": "alex.masterow@gmail.com"
  }],
  "keywords": [
    "mail",
    "mailer",
    "email",
    "symfony",
    "bundle"
  ],
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "phpunit"
  },
  "autoload": {
    "psr-4": {
      "AlexMasterov\\GenkgoMailBundle\\": "src/"
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "AlexMasterov\\GenkgoMailBundle\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "7.1 - 7.3",
    "genkgo/mail": "^2.0",
    "symfony/config": "3.3 - 4.0",
    "symfony/dependency-injection": "3.3 - 4.0",
    "symfony/http-kernel": "3.3 - 4.0",
    "symfony/yaml": "3.3 - 4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2"
  }
}
